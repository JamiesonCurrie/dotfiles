#include "term-colours/visibone"

*transparent: true
*shading: 25

! Xft font settings - used by urxvt
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:	true
Xft.hintstyle:  hintslight

URxvt*depth:                            32
*saveLines:                        4096
URxvt*lineSpace:                        0
URxvt*letterSpace:                      -1
URxvt*scrollstyle:                      plain
*scrollBar:                        false
URxvt*cursorBlink:                      true
URxvt*pointerBlank:                     true
! URxvt*cursorUnderline:                  true

URxvt*loginShell:                       true
URxvt*iso14755:                         false
URxvt*geometry:                         110x30+100+100
URxvtDesktop*geometry:                  110x30+5+5
URxvt*internalBorder:                   0

URxvt*url-launcher:                     /usr/bin/chromium

URxvt*font:                             xft:inconsolata:pixelsize=13
URxvt*boldFont:                         xft:inconsolata:bold:pixelsize=13
URxvt*italicFont:                       xft:inconsolata:italic:pixelsize=13
URxvt*boldItalicFont:                   xft:inconsolata:bold:italic:pixelsize=13

dmenu.font:          inconsolata:size=11
dmenu.selforeground: #FFCC99
dmenu.selbackground: #CC6666

XTerm*faceName: Inconsolata
XTerm*faceSize: 11
XTerm*locale:   true

URxvt*perl-ext-common:                  default,matcher,selection-to-clipboard

!URxvt*cursorColor:                      #93a1a1
!URxvtDesktop*fading:                    0
!URxvt*fading:                           40
!URxvt*fadeColor:                        #002b36

! URxvt*termName:                       string
! URxvtQuake*geometry:                    110x20
